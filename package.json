{
  "name": "11ty-podcast-starter",
  "version": "0.0.1",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "build:css": "postcss --watch src/static/css/index.css --no-map --dir dist/static/css",
    "dev": "npx eleventy --watch --serve",
    "develop": "npx eleventy --watch --serve",
    "now-build": "npm run build",
    "debug": "DEBUG=* npx eleventy",
    "postbuild": "postcss dist/static/css/index.css -r --no-map",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all --parallel watch:*",
    "build": "npm-run-all postbuild build:eleventy",
    "clean": "rimraf ./dist",
    "11ty": "npx eleventy",
    "test2": "concurrently \"npm run 11ty\" \"npm run postbuild\" \"npm run develop\""
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "concurrently": "^7.3.0",
    "eleventy-xml-plugin": ">=0.1.0",
    "js-yaml": ">=4.0.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "music-metadata": ">=7.8.1",
    "open-props": "^1.4.14",
    "rss": ">=1.2.2"
  },
  "devDependencies": {
    "cssnano": "^5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-cli": "^9.1.0",
    "postcss-custom-media": "^8.0.2",
    "postcss-import": "^14.0.0",
    "postcss-jit-props": "^1.0.7",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.4.2"
  }
}
